- content_for :description do
  = render "description", category: @category
- content_for :meta_tags do
  = display_meta_tags(category_meta_tags(@category))
- content_for :tag_ranking do
  = render "application/common/tag_ranking", tags: @tag_ranking
- breadcrumb :category, @category

ul.c-list
  = render partial: "application/common/topics", collection: @topics, as: :topic
